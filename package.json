{
  "name": "n8n-nodes-setforth",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for the Setforth (FORTH) API providing 38 resources and 150+ operations for debt resolution CRM, payment processing, e-signatures, and credit reporting.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-setforth.git"
  },
  "homepage": "https://velobpa.com",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "setforth",
    "forth",
    "debt-resolution",
    "debt-settlement",
    "crm",
    "fintech",
    "payment-processing",
    "e-signature",
    "credit-reporting",
    "clixsign",
    "forth-credit"
  ],
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "dist/nodes/Setforth/Setforth.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SetforthOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Setforth/Setforth.node.js",
      "dist/nodes/Setforth/SetforthTrigger.node.js"
    ]
  },
  "scripts": {
    
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts",
    "lint:fix": "eslint nodes credentials --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "^1.22.0"
  }
}
